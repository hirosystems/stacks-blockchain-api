{
  "title": "BnsNamesOwnByAddressResponse",
  "description": "Retrieves a list of names owned by the address provided.",
  "additionalProperties": false,
  "required": ["names"],
  "type": "object",
  "properties": {
    "names": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^([a-z0-9-_.+]{3,37})$"
      }
    }
  }
}

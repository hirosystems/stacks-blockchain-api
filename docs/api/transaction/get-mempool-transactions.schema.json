  "title": "MempoolTransactionListResponse",
  "type": "object",
  "required": ["results", "limit", "offset", "total"],
  "properties": {
    "limit": {
      "type": "integer"
    },
    "offset": {
      "type": "integer"
    },
    "total": {
      "type": "integer"
    },
    "results": {
      "type": "array",
      "items": {

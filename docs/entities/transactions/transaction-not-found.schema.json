{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "TransactionNotFound",
  "description": "This object returns the id for not found transaction",
  "additionalProperties": false,
  "properties": {
    "found": {
      "type": "boolean",
      "enum": [false]
    },
    "result": {
      "type": "object",
      "required": ["tx_id"],
      "additionalProperties": false,
      "properties": {
        "tx_id": {
          "type": "string"
        }
      }
    }
  },
  "required": ["found", "result"]
}

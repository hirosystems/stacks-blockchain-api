  },
  "devDependencies": {
    "@actions/core": "1.6.0",
    "@actions/exec": "1.1.0",
    "@actions/github": "2.2.0",
    "@actions/io": "1.1.1",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "10.0.0",
    "@stacks/eslint-config": "1.2.0",
    "@stacks/prettier-config": "0.0.10",
